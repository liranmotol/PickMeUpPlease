@model IEnumerable<MobileApplication.Models.StudentModel>

<script>
    $(function () {
  
        function udpateListValues()
        {
            console.log("udpateListValues");
            var allLi = PartialStudentList.GetAllLi();

            $(allLi).each(function (index, element)
            {
                var studentId = $(element).find('a').data("studentid");
                $(element).find('a').attr('href', 'StudentsInfoList?StudentId=' + studentId);

            });

        }
        udpateListValues();    
    });

</script>


@Html.Partial("~/Views/Students/PartialStudentList.cshtml", Model);

